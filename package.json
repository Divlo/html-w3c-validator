{
  "name": "html-w3c-validator",
  "version": "0.0.0-development",
  "public": true,
  "description": "CLI for validating multiple html pages using validator.w3.org.",
  "author": "Divlo <contact@divlo.fr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Divlo/html-w3c-validator.git"
  },
  "bugs": {
    "url": "https://github.com/Divlo/html-w3c-validator/issues"
  },
  "homepage": "https://github.com/Divlo/html-w3c-validator#readme",
  "keywords": [
    "cli",
    "html-validator",
    "w3c",
    "validator.w3.org"
  ],
  "main": "build/index.js",
  "bin": "build/index.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rimraf ./build && swc ./src --out-dir ./build && tsc",
    "lint:commit": "commitlint",
    "lint:editorconfig": "editorconfig-checker",
    "lint:markdown": "markdownlint \"**/*.md\" --dot --ignore-path \".gitignore\"",
    "lint:typescript": "eslint \"**/*.{js,jsx,ts,tsx}\" --ignore-path \".gitignore\"",
    "lint:prettier": "prettier \".\" --check --ignore-path \".gitignore\"",
    "lint:staged": "lint-staged",
    "test": "jest",
    "release": "semantic-release",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "clipanion": "3.1.0",
    "html-validator": "6.0.0",
    "ora": "5.4.1",
    "read-pkg": "5.2.0",
    "table": "6.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@swc/cli": "0.1.55",
    "@swc/core": "1.2.144",
    "@swc/jest": "0.2.17",
    "@types/html-validator": "5.0.2",
    "@types/jest": "27.4.1",
    "@types/mock-fs": "4.13.1",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "editorconfig-checker": "4.0.2",
    "eslint": "8.9.0",
    "eslint-config-conventions": "1.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-unicorn": "41.0.0",
    "execa": "5.1.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-mock-extended": "2.0.4",
    "jest-ts-webcompat-resolver": "1.0.0",
    "lint-staged": "12.3.4",
    "markdownlint-cli": "0.31.1",
    "mock-fs": "5.1.2",
    "pinst": "3.0.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "serve": "13.0.2",
    "typescript": "4.5.5"
  }
}
